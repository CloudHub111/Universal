local Mod={}
local Char=game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
game.Players.LocalPlayer.CharacterAdded:Connect(function(v)Char=v end)

function Mod.GetEveryFunction()
local Func={}
for i in pairs(Mod)do
table.insert(Func,i)
end
return table.concat(Func,"\n")
end

function Mod.GetCharacter()
return Char
end

function Mod.FindChild(Parent,Child,Method,Wait)
local TimeWait=Wait or 0
local Tick=tick()
if Parent and Method then
if Method=="WaitForChild" then
return Parent:WaitForChild(Child,TimeWait)
else
repeat task.wait()
Parent[Method](Parent,Child)
if tick()-Tick>=TimeWait then
break
end
until Parent:FindFirstChild(Child)
return Parent:FindFirstChild(Child)
end
end
end

return Mod
